{{ header }}
{{ column_left }}
{{ column_right }}

<div id="content">
    {{ content_top }}

    <div class="breadcrumb">
        {% for breadcrumb in breadcrumbs %}
        {{ breadcrumb.separator }}<a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a>
        {% endfor %}
    </div>

    <h1>{{ heading_title }}</h1>

    {% if feedbacks %}
    <div class="content">
        {% for feedback in feedbacks %}
        <div>
            <div class="name">Name: {{ feedback.feedback_author }}</div>
            <div class="description">{{ feedback.description }}</div>
        </div>
        {% endfor %}
    </div>
    {% else %}
    <div class="content">{{ text_empty }}</div>
    <div class="buttons">
        <div class="right">
            <a href="{{ continue }}" class="button">{{ button_continue }}</a>
        </div>
    </div>
    {% endif %}

    {{ pagination }}
</div>

{{ content_bottom }}
{{ footer }}
